{
    "$schema": "https://vega.github.io/schema/vega/v5.json",
    "width": 800,
    "height": 600,
    "projection": {
        "type": "equalEarth"
    },
    "data": [
        {
            "name": "mapData",
            "url": "output.json",
            "format": {
                "type": "topojson",
                "feature": "gadm41_MYS_1"
            }
        }
    ],
    "mark": 
        {
            "type": "geoshape",
            "from": {"data": "mapData"},
            "encode": {
                "enter": {
                    "fill": {"value": "#ccc"},
                    "stroke": {"value": "#333"}
                }
            }
        }
}